generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")   // pooled (pooler URL)
  directUrl = env("DIRECT_URL")     // direct (non-pooler URL)
}


model TailoredRun {
  id        String   @id @default(cuid())
  resume    String
  jd        String
  summary   String
  message   String
  createdAt DateTime @default(now())
}