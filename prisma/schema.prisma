// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model History {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())

  title     String
  jd        String
  resume    String
  tailored  String
}
